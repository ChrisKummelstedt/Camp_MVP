

<h2>
 <%= @project.title %>
</h2>
<h3>
 <%= image_tag @project.image.url(:thumb) %>
</h3>
<p>
 <%= @project.description %>
</p>
<% @project.teams.each do |team| %>
  <p> <%= link_to team.title, project_team_path(@project, team) %> </p>
  <p> <%= team.description %> </p>
<% end %>
<%= link_to "Create a Team", new_project_team_path(@project) %>
